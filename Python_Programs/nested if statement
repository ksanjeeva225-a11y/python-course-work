1. score=85
if score >=90:
    print("Grade A")
elif score >=80:
    print("Grade B")
    if score >=85:
        print("Good Job!")
elif score >=70:
    print("Grade C")
else:
    print("Grade: Below c")


2.num=int(input("Enter a number: "))
